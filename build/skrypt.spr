// Test sterowania jamnikiem
#define IDZ Move

// Start
Pause 1000

// Ustaw jamnika w przestrzeni
Set Jamnik 0 0 0 0 0 0

// Jamnik idzie do przodu
IDZ Jamnik 5 4

Pause 800

// Jamnik merda ogonem
Rotate Jamnik.Ogon OY 20 45
Rotate Jamnik.Ogon OY 20 -45
Rotate Jamnik.Ogon OY 20 45
Rotate Jamnik.Ogon OY 20 -45

Pause 500

// Podnosi lewą przednią łapę
Rotate Jamnik.LapaLF OX 15 40
Pause 300
Rotate Jamnik.LapaLF OX 15 -40

Pause 900

// Wraca
Move Jamnik -5 4
